{
  "name": "erliz/owp-client",
  "description": "PHP client for OpenVZ web panel remote API",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/Erliz/owp-client",
  "keywords": [
    "openvz",
    "openvz-web-panel",
    "owp-api"
  ],
  "authors": [
    {
      "name": "Stanislav Vetlovskiy",
      "email": "mrerliz@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.3",
    "symfony/var-dumper": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Erliz\\OwpClient\\": "src/Erliz/OwpClient/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Erliz\\OwpClient\\": "tests/Erliz/OwpClient/"
    }
  },
  "minimum-stability": "stable"
}
